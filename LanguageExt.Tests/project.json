  "configurations": {
    "Debug": {
      "compilationOptions": {
        "define": [ "DEBUG", "TRACE" ]
      }
    },
    "Release": {
      "compilationOptions": {
        "define": [ "RELEASE", "TRACE" ],
        "optimize": true
      }
    }
  },
  "dependencies": {
    "LanguageExt.Core": "1.8.3-alpha",
    "LanguageExt.Parsec": "1.8.3-alpha",
    "LanguageExt.Process": "1.8.3-alpha",
    "LanguageExt.Process.FSharp": "1.8.3-alpha",
    "LanguageExt.Process.Redis": "1.8.3-alpha",
    "LanguageExt.FSharp": "1.8.3-alpha",
    "LanguageExt.ProcessJS": "1.8.3-alpha",
    "Newtonsoft.Json": "7.0.1",
    "Rx-Main": "2.2.5.0",
    "StackExchange.Redis": "1.0.488",
    "FSharp.Core": "4.0.0.1",
    "xunit.assert": "2.1.0-*",
    "xunit.extensibility.execution": "2.1.0-*"
  },
  "commands": {
    "test": "xunit.runner.aspnet"
  },
  "frameworks": {
    "dnx451": {
      "dependencies": {
        "Microsoft.Dnx.Compilation.CSharp": "1.0.0-*",
        "xunit.runner.aspnet": "2.0.0-aspnet-*"
      },
      "frameworkAssemblies": {
        "System.Runtime": "4.0.10.0",
        "System.Threading.Tasks": "4.0.0.0"
      }
    }
  },
  "compile": "*.cs"
}